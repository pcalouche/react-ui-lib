{
  "name": "@pcalouche/react-ui-lib",
  "version": "1.0.0",
  "description": "Example React UI Library built with Rollup.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pcalouche/react-ui-lib.git"
  },
  "author": "Philip Calouche",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "esnext": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "bootstrap": "4.4.1",
    "cpx": "1.5.0",
    "cross-env": "5.2.1",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "reactstrap": "8.4.1",
    "rimraf": "2.7.1",
    "rollup": "1.31.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-img": "1.1.0",
    "rollup-plugin-size-snapshot": "0.11.0",
    "rollup-plugin-terser": "5.2.0",
    "webpack": "4.41.5"
  },
  "peerDependencies": {
    "bootstrap": "4.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "reactstrap": "8.4.1"
  },
  "scripts": {
    "scss:watch": "cpx \"src/scss/**\" src/scss -v -w",
    "image:watch": "cpx \"src/images/*.{png,jpg}\" src/images -v -w",
    "src:watch": "cpx \"src/**\" dist/src -v -w",
    "rollup:watch": "rollup -c -w",
    "build:watch": "npm-run-all -p scss:watch image:watch src:watch rollup:watch",
    "clean": "rimraf dist",
    "scss:copy": "cpx \"src/scss/**'\" dist/scss -v",
    "image:copy": "cpx \"src/images/*.{png,jpg}\" dist/images -v",
    "src:copy": "cpx \"src/**\" dist/src -v",
    "rollup:build": "rollup -c",
    "build": "npm-run-all -s clean scss:copy image:copy src:copy rollup:build",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
